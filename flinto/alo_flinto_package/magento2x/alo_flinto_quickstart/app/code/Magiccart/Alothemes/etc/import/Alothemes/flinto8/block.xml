<?xml version="1.0" encoding="UTF-8"?>
<root>
  <block>
    <item>
      <title><![CDATA[Custom Product Detail]]></title>
      <identifier><![CDATA[additional-custom-product]]></identifier>
      <content><![CDATA[<div class="container">
    <div class="block-custom-product">
		<div class="row">
			<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 custom-product custom1">
				{{widget type="Magiccart\Magicproduct\Block\Widget\Product" identifier="custom-product-new" template="productcustom.phtml"}}
			</div>
			<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 custom-product custom2">
				{{widget type="Magiccart\Magicproduct\Block\Widget\Product" identifier="custom-product-featured" template="productcustom.phtml"}}
			</div>
			<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 custom-product custom3">
				 {{widget type="Magiccart\Magicproduct\Block\Widget\Product" identifier="custom-product-sale" template="productcustom.phtml"}}
			</div>
			<div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 custom-product custom4">
				{{widget type="Magiccart\Magicproduct\Block\Widget\Product" identifier="custom-product-latest" template="productcustom.phtml"}} 
			</div>
		</div>
	</div>
</div>]]></content>
      <is_active><![CDATA[1]]></is_active>
    </item>
    <item>
      <title><![CDATA[additional-complete-policy-block]]></title>
      <identifier><![CDATA[additional-complete-policy-block]]></identifier>
      <content><![CDATA[
<div class="alo-theme complete-static-policy policy">
    <div class="container">
		<div class="policy-wrap clearfix">
			<div class="policy-box">
				<div class="box-content">
					<div class="service-content">
					    <img class="img-responsive" src="{{media url="wysiwyg/alothemes/static/service1.png"}}" alt="img" />
						<div class="info-title">
							<h3>Free Shipping</h3>
						</div>
					</div>
				</div>
			</div>
			<div class="policy-box">
				<div class="box-content">
					<div class="service-content">
					    <img class="img-responsive" src="{{media url="wysiwyg/alothemes/static/service2.png"}}" alt="img" />
						<div class="info-title">
							<h3>Support Customer</h3>
						</div>
					</div>
				</div>
			</div>
			<div class="policy-box">
				<div class="box-content">
					<div class="service-content">
					    <img class="img-responsive" src="{{media url="wysiwyg/alothemes/static/service3.png"}}" alt="img" />
						<div class="info-title">
							<h3>Secure Payments</h3>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>]]></content>
      <is_active><![CDATA[1]]></is_active>
    </item>
    <item>
      <title><![CDATA[contact_details]]></title>
      <identifier><![CDATA[contact_details]]></identifier>
      <content><![CDATA[<div class="contact_details">
    <p class="short-description">If you need any help, please contact us <br> or send us an email</p>
	<div class="contact_info">
		<div class="row no-margin">
		    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 no-padding">
				<div class="info_main border_left">
					<div class="ion-android-pin icons"><span class="hidden">hidden</span></div>
					<div class="flex">
						<p class="tit-name">Address</p>
						<p class="tit-contain">5 South Main Street,<br>Englishtown, NJ 07726</p>
					</div>
				</div>
			</div>
			<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 no-padding">
			    <div class="info_main border_right">
					<div class="ion-android-call icons"><span class="hidden">hidden</span></div>
					<div class="flex">
						<p class="tit-name">Phone</p>
						<p class="tit-contain">+1 (078) 575 91 81</p>
						<p class="tit-contain">+1 (078) 575 81 91</p>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>]]></content>
      <is_active><![CDATA[1]]></is_active>
    </item>
    <item>
      <title><![CDATA[Banner menu Shop]]></title>
      <identifier><![CDATA[banner-menu-shop]]></identifier>
      <content><![CDATA[<div class="cms-static-banner-menu">
	<div class="banner-menu">
		<a href="#"><img class="img-responsive" src="{{media url="wysiwyg/alothemes/static/Elements_Mega_Menu.jpg"}}" alt="img Menu" /></a>
	</div>
</div>]]></content>
      <is_active><![CDATA[1]]></is_active>
    </item>
    <item>
